{
  "name": "app-contentstudio-plus",
  "version": "1.7.0",
  "description": "Enterprise extension for Content Studio",
  "license": "Enonic License 1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/enonic/app-contentstudio-plus"
  },
  "scripts": {
    "build": "concurrently -c auto -g --timings npm:build:main:*",
    "build:main:static": "tsup -d build/resources/main/_static",
    "build:main:server": "tsup -d build/resources/main",
    "build:main:styles": "webpack --color",
    "check:types": "concurrently -c auto -g --timings npm:check:types:*",
    "check:types:static": "tsc --pretty --skipLibCheck --noEmit -p src/main/resources/_static/tsconfig.json",
    "check:types:server": "tsc --pretty --skipLibCheck --noEmit -p src/main/resources/tsconfig.json",
    "lint": "eslint src/**/*.ts",
    "lint:quiet": "eslint --quiet src/**/*.ts",
    "fix": "eslint --fix src/**/*.ts"
  },
  "dependencies": {
    "@enonic/legacy-slickgrid": "v0.0.2",
    "@enonic/lib-admin-ui": "file:./.xp/dev/lib-admin-ui",
    "jquery": "^3.7.1",
    "jquery-ui-dist": "^1.13.2",
    "lib-contentstudio": "file:./.xp/dev/lib-contentstudio",
    "normalize-url": "^8.0.0",
    "q": "^1.5.1"
  },
  "devDependencies": {
    "@enonic-types/global": "^7.13.3",
    "@enonic-types/lib-admin": "^7.13.3",
    "@enonic-types/lib-content": "^7.13.3",
    "@enonic-types/lib-i18n": "^7.13.3",
    "@enonic-types/lib-io": "^7.13.3",
    "@enonic-types/lib-portal": "^7.13.3",
    "@enonic/esbuild-plugin-copy-with-hash": "^0.1.0",
    "@enonic/eslint-config": "^1.2.0",
    "@enonic/tsup-plugin-manifest": "^0.0.1",
    "@swc/core": "^1.3.93",
    "@types/jquery": "^3.5.22",
    "@types/jqueryui": "^1.12.18",
    "@types/normalize-url": "^4.2.0",
    "@types/q": "^1.5.6",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.16",
    "browserslist": "^4.22.1",
    "browserslist-config-enonic": "^1.0.7",
    "circular-dependency-plugin": "^5.2.0",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "enonic-admin-artifacts": "^1.8.5",
    "error-logger-webpack-plugin": "^1.1.1",
    "esbuild-plugin-external-global": "^1.0.1",
    "esbuild-plugin-globals": "^0.2.0",
    "eslint": "^8.51.0",
    "file-loader": "^6.2.0",
    "glob": "^10.3.10",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-normalize": "^10.0.1",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.0.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.5.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "browserslist": [
    "extends browserslist-config-enonic"
  ],
  "engines": {
    "node": ">= 18.17.1",
    "npm": ">= 9.6.7"
  }
}
